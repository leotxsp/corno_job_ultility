{
  "name": "Pdf Auto naming",
  "version": "1.0",
  "description": "Automatically names PDF files based on a DOM tag.",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "storage", "downloads"],
  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
    "js": ["content.js"]
  }
]

}